class Solution {
    static int digitcount(int n){
        int dcount=0;
        while(n>0){
            n=n/10;
            dcount++;
        }
        return dcount;
    }
    public int findNumbers(int[] nums) {
        int count=0;
        for(int i=0;i<nums.length;i++){
            if(digitcount(nums[i])%2==0){
                count++;
            }
        }
        return count;
    }
}
